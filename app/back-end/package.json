{
  "name": "pasta-so-tasty-back-end",
  "version": "1.0.0",
  "description": "Back-end da aplicação Pasta so tasty",
  "main": "./build/server",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "(npx -y tsc && npx sequelize db:create && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all)",
    "start": "nodemon",
    "debug": "nodemon --watch src --ext ts --exec ts-nodemon src",
    "lint": "eslint .",
    "dev": "nodemon --watch \"./src/**\" ./src/server.ts -L",
    "restart": "(npx sequelize-cli db:seed:undo:all && npx sequelize-cli db:migrate:undo:all)"
  },
  "keywords": [],
  "author": "Fernando Bissi",
  "license": "ISC",
  "dependencies": {
    "@types/bcryptjs": "^2.4.2",
    "bcrypt": "^5.1.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "joi": "^17.9.2",
    "jsonwebtoken": "^8.5.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.14",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.28.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.5.1",
    "eslint-plugin-promise": "^6.1.1",
    "mysql2": "^2.3.3",
    "nodemon": "^2.0.20",
    "sequelize": "^6.26.0",
    "sequelize-cli": "^6.5.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
  }
}
